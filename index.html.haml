            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand{href: 'http://devinbarham.com'} Devin Barham

        .collapse.navbar-collapse#bs-example-navbar-collapse-1
          %ul.nav.navbar-nav
            %li
              %a{href: '#'} About Me
            %li
              %a{href: '#'} Résumé
            %li.dropdown
              %a.dropdown-toggle{'data-toggle' => 'dropdown', href: '#'}
                Projects
                %b.caret
              %ul.dropdown-menu
                %li
                  %a{href: '#'} Phonegap
                %li
                  %a{href: '#'} Things
